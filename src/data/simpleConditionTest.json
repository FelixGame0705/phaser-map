{
  "version": "1.0.0",
  "programName": "simple_condition_test",
  "actions": [
    {
      "type": "repeatRange",
      "variable": "i",
      "from": 1,
      "to": 5,
      "step": 1,
      "body": [
        { "type": "forward", "count": 1 },
        {
          "type": "if",
          "cond": {
            "type": "variableComparison",
            "variable": "i",
            "operator": "==",
            "value": 3
          },
          "then": [
            { "type": "collect", "color": "green", "count": "{{i}}" }
          ]
        }
      ]
    }
  ]
}
