[{
    "version": "1.0.0",
    "programName": "battery_collection_demo",
    "actions": [
      { "type": "turnRight" },
      { "type": "forward", "count": 3 },
  
      { "type": "repeat", "times": 3, "body": [
        { "type": "collect", "color": "green" }
      ]}
    ]
},
{
    "version": "1.0.0",
    "programName": "square_loop",
    "actions": [
      { "type": "repeat", "times": 4, "body": [
        { "type": "forward", "count": 1 },
        { "type": "turnRight" }
      ]},
  
      { "type": "collect", "color": "yellow", "count": 2 }
    ]
  },
  {
    "version": "1.0.0",
    "programName": "if_example",
    "actions": [
      { "type": "if",
        "cond": { "sensor": "frontIsClear" },
        "then": [
          { "type": "forward", "count": 2 }
        ],
        "else": [
          { "type": "turnLeft" }
        ]
      },
  
      { "type": "collect", "color": "red", "count": 1 }
    ]
  },
  {
    "version": "1.0.0",
    "programName": "while_walk",
    "actions": [
      { "type": "while",
        "cond": { "sensor": "frontIsClear" },
        "body": [
          { "type": "forward", "count": 1 }
        ]
      },
      { "type": "turnBack" }
    ]
  },
  {
    "version": "1.0.0",
    "programName": "conditional_collect",
    "actions": [
      { "type": "if",
        "cond": { "sensor": "atBattery", "args": ["green"] },
        "then": [
          { "type": "collect", "color": "green", "count": 1 }
        ],
        "else": [
          { "type": "turnRight" }
        ]
      }
    ]
  },
  {
    "version": "1.0.0",
    "programName": "repeat_range_demo",
    "actions": [
      {
        "type": "repeatRange",
        "variable": "i",
        "from": 1,
        "to": 3,
        "step": 1,
        "body": [
          { "type": "forward", "count": "{{i}}" },
          { "type": "turnRight" }
        ]
      },
      {
        "type": "repeatRange",
        "variable": "step",
        "from": 2,
        "to": 6,
        "step": 2,
        "body": [
          { "type": "forward", "count": "{{step}}" },
          { "type": "collect", "color": "green", "count": 1 }
        ]
      }
    ]
  }
  
]